pipeline {
  agent any
  stages {
    stage ('Build') {
      steps {
           
            sh ''' 
             mvn clean install -DskipTests
            cd /var/jenkins_home/workspace/Avinash_pru_main/target/
            ls -laRt
            '''
      }
    }
    stage ('Deploy') {
      steps {
          sh 'java -jar /var/jenkins_home/workspace/Avinash_pru_main/target/spring-boot-microservice 0.0.1-SNAPSHOT'

      }
    }
  }
}
