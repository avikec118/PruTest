pipeline {
    #agent { docker { image 'maven:3.6.1-jdk-8-alpine' } }
    stages {
        stage('build') {
            steps {
                checkout poll: false, scm: [$class: 'GitSCM', branches: [[name: "master"]], extensions: [[$class: 'RelativeTargetDirectory', relativeTargetDir: 'services']], userRemoteConfigs: [[ url: 'git@github.com:avikec118/PruTest.git']]]
            }
        }
    }
}
